String myPhone = '123'
String myName = 'bot'
String myEmail = 'bot@company.name'

(1..100).each {
  String location = "location-${it}"
  int myPrice = 2
  int mySqft = 1 + (it as int)
  def rating = rating location, myPrice, mySqft

  if (rating > 400) {
    apartment(location) {
      price myPrice
      sqft mySqft
      phone myPhone
      realtorName myName
      mail myEmail
    }
  }
}
